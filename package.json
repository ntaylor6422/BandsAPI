{
  "name": "cc17api",
  "version": "0.0.1",
  "type": "module",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/node": "^8.0.29",
    "nodemon": "^2.0.7",
    "ts-node": "^3.3.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "apollo-server": "^2.19.2",
    "body-parser": "^1.19.0",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.10",
    "type-graphql": "^1.1.1",
    "typeorm": "0.2.30",
    "typeorm-seeding": "^1.6.1"
  },
  "scripts": {
    "start": "nodemon -w src --ext ts --exec ts-node src/index.ts",
    "typeorm:migration:gen": "ts-node ./node_modules/typeorm/cli.js migration:generate --name",
    "typeorm:migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run"
  }
}
